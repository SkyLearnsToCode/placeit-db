<!doctype html>

<html>
<head>
  <title>WebReg Pro by Yellow Fellows</title> 
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
  <link href="/css/introHCI.css" rel="stylesheet">
  <!-- this is a comment in HTML -->

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->     
<script src="https://code.jquery.com/jquery.js"></script>
<script src="/js/bootstrap.min.js"></script>   
<script src="/js/knockout.js"></script>
<script src="/js/socket.io/dist/socket.io.js"></script>
<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="/js/gradeChart.js"></script>
<script src="/js/Models/InfoModel.js"></script>
<script src="/js/ViewModels/InfoViewModel.js"></script>
</head>
<script>
$(document).ready(function(){
  var ViewModel = new InfoViewModel(InfoModel, document.getElementById("container"));
  ViewModel.initialize();
});
</script>
<body class="container" id="container">
  <div class="page-header row ">
    <h1> 
    <img width="70" height="70" src="http://after5chronicles.files.wordpress.com/2013/02/ouroboros042.gif"/>
      Class Tracker <small data-bind="text : _class().name"></small>
    </h1>
  </div>
    <ol class="breadcrumb">
      <li data-bind="text : _class().group"></li>
      <li class="active" data-bind="text : _class().name"></li>
    </ol>

    <h3>Presented by <span data-bind="text : _class().instructor"></span></h3>
    <span class="label label-primary">Lecture Times</span>
    <table class="table">
      <thead><tr>
        <td>Day</td> <td>Hour</td>
      </tr></thead>

      <tbody>
        <tr><td>Monday</td><td>5:00 - 5:50</td></tr>
        <tr><td>Wednesday</td><td>5:00 - 5:50</td></tr>
        <tr><td>Friday</td><td>5:00 - 5:00</td></tr>
      </tbody>
    </table>
    <span class="label label-primary">Discussion Times</span>
    <table class="table">
      <thead><tr>
        <td>Day</td> <td>Hour</td>
      </tr></thead>
      <tbody>
        <tr><td>Tuesday</td><td>9:00 - 9:50</td></tr>
        <tr><td>Thursday</td><td>3:00 - 4:50</td></tr>
      </tbody>
    </table>
    <span class="label label-primary">Prerequisites</span>
      <ul class="list-group">
         <li class="list-group-item">CSE 100</li>
         <li class="list-group-item">CSE 105</li>
    </ul>

    <button class="btn-info">View CAPE evaluations</button>

  <div class="container">

    <div class="row">
      <div class="col-md-8" data-bind="gradechart : ''"></div>
      <div class="col-md-4">
        <div class="page-header row ">
          <h2>Messages</h2>
          <ul class="list-group" >
            <!-- ko if : messages().length == 0 -->
            <li class="list-group-item">No messages posted.</li>
            <!-- /ko -->
            <!-- ko foreach : messages-->
            <li class="list-group-item" data-bind="text : $data"></li>
            <!-- /ko-->
          </ul>
          <textarea type="text" data-bind="value : currentMessage" class="form-control" placeholder="Place message here"></textarea><br>
          <button data-bind="submitMessage : currentMessage"class="btn btn-success">Submit</button>
        </div>
    </div>
  </div>

</body>



</html>
